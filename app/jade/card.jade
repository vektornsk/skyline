extends ./tmp/_template.jade
include ./tmp/_bread-crumbs.jade

block title
	title Card
block content
	.wrap-top
		.container.content-wrap
			.content-top
				+bread
			.card-wrap
				.card__right
					.card__inner
						.card-sale СКИДКА 20%
						.card-nal Нет в наличии
						h1 LEVANTE SOLARE 60 УЛЬТРАТОНКИЕ ЛЕТНИЕ
						.info-product
							p Комфортные матовые колготки из плотной микрофибры
							p <span class="bold">Состав:</span> 90% ХЛОПОК, 10% ПОЛИАМИД, 10% ЭЛАСТАН
							.m-product-img
								img(src="/images/product-1.png")
						form
							.card-setting
								.card-setting__left
									.card-setting-top
										select.select.select(placeholder="Размер: 4")
											option
											option(value="1") Размер: 6
											option(value="2") Размер: 12
											option(value="3") Размер: 18
											option(value="4") Размер: 24
										a.tbl-size.popup-img(href="/images/size.jpg") Таблица размеров

									select.select(placeholder="Плотность: 20 ден")
										option
										option(value="1") Плотность: 20 ден
										option(value="2") Плотность: 22 ден
										option(value="3") Плотность: 23 ден
										option(value="4") Плотность: 24 ден
									.card-color
										.card-color__title Цвет:
										ul.product-color
											li.product-color__item
												label.color
													input(type="radio", name="color", checked)
													span.color-el.color-1(title="Цвет")
											li.product-color__item
												label.color
													input(type="radio", name="color")
													span.color-el.color-2(title="Цвет")
											li.product-color__item
												label.color
													input(type="radio", name="color")
													span.color-el.color-3(title="Цвет")
											li.product-color__item
												label.color
													input(type="radio", name="color")
													span.color-el.color-4(title="Цвет")
											li.product-color__item
												label.color
													input(type="radio", name="color")
													span.color-el.color-5(title="Цвет")

								.card-setting__right
									.card-col Количество:
										.btn-calc
											.btn-calc__left
												span 1
												input(type="text", value="1", name="")
											.btn-calc__right
												.btn-calc__plus
												.btn-calc__minus
									.card-price
										.card-price__num 750 руб.
										.card-price__num-sale 850 руб.
										// <div class="card-price__num-one">1 шт - 750 руб.</div>
									.card-btn
										input.btn(type="submit", value="Добавить в карзину")
